{% extends 'base.html.twig' %}

{% block title %}Circuit{% endblock %}

{% block body %}
    <section id="show-circuit">
        <div class="show-circuit-header">
            <div class="show-circuit-title">
                <image src="/image//uploads/circuits/{{circuit.image}}" class="show-circuit-banner">
                <h1>{{ circuit.title }}</h1>
                <p>{{circuit.stage}}</p>
            </div>
            <div class="show-circuit-icone">
                <div class="circuit-icone">
                    <img src="/image/relationship-icone.svg" loading="lazy" alt="">
                    <p>{{ circuit.relationship }}</p>
                </div>
                <div class="circuit-icone-down">
                    <div class="circuit-icone">
                        <img src="/image/duration-icone.svg" loading="lazy" alt="">
                        <p>{{ circuit.duration }}</p>
                    </div>
                    <div class="circuit-icone">
                        <img src="/image/price-icone.svg" loading="lazy" alt="">
                        <p>{{ circuit.price }}</p>
                    </div>
                </div>
                <p class="btn-fix">
                    <a href="">Je suis intéressé</a>
                </p>
            </div>
        </div>
        <div class="show-circuit-container">
            <p class="show-circuit-fullcontent">{{ circuit.fullContent }}</p>
